{
  "expo": {
    "name": "Smile",
    "slug": "smile",
    "version": "1.0.0",

    "orientation": "portrait",
    "scheme": "smile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,

    "icon": "./assets/notification-icon.png",

    "runtimeVersion": {
      "policy": "appVersion"
    },

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "package": "com.farmerintech.smile",
      "googleServicesFile": "./google-services.json",
      "edgeToEdgeEnabled": true,

      "adaptiveIcon": {
        "foregroundImage": "./assets/notification-icon.png",
        "backgroundColor": "#093131"
      }
    },

    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },

    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-font",

      [
        "expo-splash-screen",
        {
          "image": "./assets/notification-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#093131"
        }
      ],

      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#ffffff",
          "defaultChannel": "default"
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true
    },

    "extra": {
      "router": {},
      "eas": {
        "projectId": "283b5794-dc09-4616-8cc9-91ffc579cd54"
      }
    },

    "owner": "farmerintech"
  }
}
